-  // update the state of all opened joysticks
-  SDL_JoystickUpdate();
-  int axisNum;
-  int buttonIdx = -1, buttonNum;
-  int hatIdx = -1, hatNum;
-
-  // go through all joysticks
-  std::map<int, SDL_Joystick*>::const_iterator it;
-  for (it = m_Joysticks.begin(); it != m_Joysticks.end(); ++it)

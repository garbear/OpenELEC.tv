+#ifndef KODI_GAME_TYPES_H_
+#define KODI_GAME_TYPES_H_
+
+/* current game API version */
+#define GAME_API_VERSION                "1.0.13"
+
+/* min. game API version */
+#define GAME_MIN_API_VERSION            "1.0.13"

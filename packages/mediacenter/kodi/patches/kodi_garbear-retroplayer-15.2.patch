+
+  private:
+    /*!
+     * @brief Helper functions
+     */
+    static void GetPeripheralInfo(const CPeripheral* device, ADDON::Peripheral& peripheralInfo);
+    static void SetPeripheralInfo(CPeripheral& device, const ADDON::Peripheral& peripheralInfo);
+

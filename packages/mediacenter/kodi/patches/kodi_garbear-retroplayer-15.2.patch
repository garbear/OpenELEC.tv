-void CJoystick::SetEnabled(bool enabled /*=true*/)
-{
-  if( enabled && !m_joystickEnabled )
-  {
-    m_joystickEnabled = true;
-    Initialize();
-  }
-  else if( !enabled && m_joystickEnabled )
-  {
-    ReleaseJoysticks();
-    m_joystickEnabled = false;
-  }
-}
